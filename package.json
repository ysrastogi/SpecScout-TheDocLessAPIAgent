{
  "name": "docless-api-agent-demo",
  "version": "1.0.0",
  "description": "A demonstration of doc-less API reverse engineering with GitHub REST API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only demo.ts",
    "demo": "node dist/demo.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/*.integration.test.js'",
    "test:postman": "newman run postman_collection.json",
    "docker:up": "cd mock-lab && docker-compose up -d",
    "docker:down": "cd mock-lab && docker-compose down",
    "generate:openapi": "swagger-codegen generate -i openapi.yaml -l typescript-node -o generated/",
    "lint": "eslint sdk/**/*.ts tests/**/*.ts --fix",
    "start": "node dist/index.js",
    "serve": "node server.js",
    "serve:dev": "nodemon server.js",
    "serve:demo": "python3 -m http.server 8080",
    "serve:nginx": "nginx -c $(pwd)/nginx.conf",
    "docs:generate": "typedoc --out docs sdk/"
  },
  "keywords": [
    "api",
    "reverse-engineering",
    "github",
    "pagination",
    "rate-limiting",
    "typescript",
    "openapi"
  ],
  "author": "API Witness Demo",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "newman": "^6.0.0",
    "@apidevtools/swagger-cli": "^4.0.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "oidc-provider": "^8.4.6"
  }
}
